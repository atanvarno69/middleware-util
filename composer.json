{
    "name": "atanvarno/middleware-util",
    "description": "PSR-15 middleware utilities",
    "keywords": [
        "PSR-15", "middleware", "utilities"
    ],
    "homepage": "https://github.com/atanvarno69/middleware-util",
    "license": "MIT",
    "authors": [
        {
            "name": "atanvarno69",
            "email": "atanvarno69@gmail.com",
            "homepage": "https://github.com/atanvarno69",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "psr/http-server-middleware": "^1.0.1",
        "psr/http-factory": "^1.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.5",
        "http-interop/http-factory-diactoros": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "Atanvarno\\Middleware\\Util\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atanvarno\\Middleware\\Util\\Test\\": "tests"
        }
    },
    "suggest": {
        "atanvarno/simple-middleware-dispatch": "Provides a basic middleware dispatcher"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
